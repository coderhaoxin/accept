{
  "name": "typed-objects",
  "version": "0.4.0",
  "description": "polyfill of typed objects for node.js",
  "main": "dest/index.js",
  "scripts": {
    "test": "mocha -R spec -t 50 test/*.js",
    "prepublish": "mkdir -p dest & babel lib/index.js -o dest/index.js",
    "test-cov": "istanbul cover node_modules/.bin/_mocha -- -R dot -t 50 test/*.js",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- -R dot -t 50 test/*.js"
  },
  "repository": "coderhaoxin/typed-objects",
  "keywords": [
    "polyfill",
    "objects",
    "structs",
    "struct",
    "typed",
    "type",
    "es7"
  ],
  "devDependencies": {
    "babel": "5",
    "istanbul-harmony": "0",
    "mocha": "2"
  },
  "files": [
    "dest",
    "lib"
  ],
  "author": "haoxin",
  "license": "MIT"
}
