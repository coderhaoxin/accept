{
  "name": "typed-objects",
  "version": "1.0.0",
  "description": "polyfill of typed objects for node.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib --out-dir dist",
    "watch": "babel lib --out-dir dist --watch",
    "webpack": "webpack --config webpack.config.js",
    "test": "mocha",
    "test-cov": "istanbul cover node_modules/.bin/_mocha",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly",
    "prepublish": "npm run build && npm run webpack"
  },
  "repository": "coderhaoxin/typed-objects",
  "keywords": [
    "polyfill",
    "objects",
    "structs",
    "struct",
    "typed",
    "type",
    "es7"
  ],
  "dependencies": {
    "object-assign": "4"
  },
  "devDependencies": {
    "babel": "5",
    "babel-core": "5",
    "babel-loader": "5",
    "istanbul-harmony": "0",
    "mocha": "2",
    "webpack": "1"
  },
  "files": [
    "release",
    "dist"
  ],
  "author": "haoxin",
  "license": "MIT"
}
